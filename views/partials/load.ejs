<script>
  window.onload = function() {
    <% for (var i = 0; i < graphs.length; i++) { %>
      <% var graph = graphs[i]; %>
      <% if (graph.type == "bar") { %>
        graphBar(
      <% } else if (graph.type == "pie") { %>
        graphPie(
      <% } else if (graph.type == "line") { %>
        graphLine(
      <% } %>
          "#<%- graph.elementId %>",
          <%- JSON.stringify(graph.xData) %>,
          <%- JSON.stringify(graph.yData) %>
        );
    <% } %>
    <% if (currPage == "calendar") { %>
      loadGCal(<%- JSON.stringify(events) %>);
    <% } else if (currPage == "policies" || currPage == "handbook") { %>
      showPDF('.pdf');
    <% } else if (currPage == "leadership") { %>
      toggleCard('expectedAbsences');
      toggleCard('assignments');
      toggleCard('scores');
    <% } %>
  }
</script>
